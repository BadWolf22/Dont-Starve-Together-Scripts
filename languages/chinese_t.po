msgstr ""

#. STRINGS.CHARACTERS.WENDY.DESCRIBE.ATRIUM_LIGHT.ON
msgctxt "STRINGS.CHARACTERS.WENDY.DESCRIBE.ATRIUM_LIGHT.ON"
msgid "A deathly light dances across the room."
msgstr ""

#. STRINGS.CHARACTERS.WENDY.DESCRIBE.ATRIUM_OVERGROWTH
msgctxt "STRINGS.CHARACTERS.WENDY.DESCRIBE.ATRIUM_OVERGROWTH"
msgid "Scrawlings of a tongue lost to time."
msgstr ""

#. STRINGS.CHARACTERS.WENDY.DESCRIBE.ATRIUM_RUBBLE.LINE_1
msgctxt "STRINGS.CHARACTERS.WENDY.DESCRIBE.ATRIUM_RUBBLE.LINE_1"
