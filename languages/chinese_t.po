msgstr ""

#. STRINGS.CHARACTERS.WEBBER.DESCRIBE.SANDSPIKE
msgctxt "STRINGS.CHARACTERS.WEBBER.DESCRIBE.SANDSPIKE"
msgid "Ouchie!"
msgstr ""

#. STRINGS.CHARACTERS.WEBBER.DESCRIBE.SANITYROCK.ACTIVE
msgctxt "STRINGS.CHARACTERS.WEBBER.DESCRIBE.SANITYROCK.ACTIVE"
